pub fn layout.SingleLayout {
	SolidBackground(color=#2b1d27)

	Spawn turf {
		Turf(color=#463441)
        StartingTurf(turf)
        FlashTurfOnVictory
		ShrinkOverTime scale {
			TurfShape = Equilateral(radius=22 * scale, numPoints=3)
		}
	}

	Spawn obstacle {
		Boulder(shape=Equilateral(numPoints=3, radius=0.9), pos=@(0,0), turningDecay=0.001)
	}
}